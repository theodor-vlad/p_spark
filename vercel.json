{
    "version": 2,
    "builds": [
        {
            "src": "my_django_app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "my_django_app/wsgi.py"
        }
    ]
}

